# Music Recognition API
# Option 1: AudD (recommended for cross-platform)
AUDD_API_KEY=your_audd_api_key_here

# Option 2: ACRCloud (alternative)
# ACRCLOUD_ACCESS_KEY=your_access_key_here
# ACRCLOUD_ACCESS_SECRET=your_access_secret_here

# Music Metadata API
MUSICBRAINZ_USER_AGENT=Allegro/1.0 (https://github.com/JMBeh/Allegro)
MUSICBRAINZ_EMAIL=your_email@example.com

# Spotify API (for "what's next" recommendations)
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
SPOTIFY_REDIRECT_URI=http://localhost:3000/api/auth/callback/spotify

# AI Enhancement (Essential - for generating coaching content)
OPENAI_API_KEY=your_openai_api_key_here
# Alternative: Use Claude API
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Genius API - Song Lyrics & Annotations (Optional)
# Get Access Token at: https://genius.com/api-clients
# Note: GENIUS_API_KEY is the Access Token from Genius
GENIUS_API_KEY=your_genius_api_key_here
